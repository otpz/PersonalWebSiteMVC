@model List<TalentViewModel>

<div class="col-lg-12" data-aos="fade-up">
    @foreach (var talent in Model)
    {
        <div class="progress">
            <span class="skill">@talent.Name <i class="val">%@talent.Progress</i></span>
            <div class="progress-bar-wrap">
                <div class="progress-bar" role="progressbar" aria-valuenow="@talent.Progress" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
        </div>
    }
</div>
